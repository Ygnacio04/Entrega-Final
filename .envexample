# Configuración del servidor
PORT = 3000
PUBLIC_URL = http://localhost:3000

# Base de datos
DB_URI = mongodb+srv://usuario:contraseña@cluster.mongodb.net/NombreBaseDeDatos?retryWrites=true&w=majority

# JWT (JSON Web Token)
JWT_SECRET = clave_secreta_jwt

# Configuración de Pinata para IPFS
PINATA_API_KEY = api_key_de_pinata
SECRET_API_KEY = clave_secreta_de_pinata
PINATA_GATEWAY = nombre_de_gateway.mypinata.cloud

# Configuración de Email con OAuth2
EMAIL = correo@gmail.com
EMAIL_USER = correo@gmail.com
EMAIL_PASSWORD = contraseña_de_aplicacion

# Google OAuth2 para envío de emails
CLIENT_ID = client_id_de_google_oauth
CLIENT_SECRET = client_secret_de_google_oauth
REFRESH_TOKEN = refresh_token_de_google_oauth
ACCESS_MAIL_TOKEN = access_token_de_google
REDIRECT_URI = https://developers.google.com/oauthplayground

# Slack para notificaciones de errores
SLACK_WEBHOOK = https://hooks.slack.com/services/tu_webhook_de_slack